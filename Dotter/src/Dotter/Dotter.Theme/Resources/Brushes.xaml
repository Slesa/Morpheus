<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Colors.xaml" />
    </ResourceDictionary.MergedDictionaries>
    
    <SolidColorBrush x:Key="Brush.Foreground" Color="{StaticResource Color.Foreground}" />
    <SolidColorBrush x:Key="Brush.Background" Color="{StaticResource Color.Primary}" />
    
    <SolidColorBrush x:Key="Brush.Highlight" Color="{StaticResource Color.Highlight}" />
    <SolidColorBrush x:Key="Brush.Highlight.1" Color="{StaticResource Color.Highlight.1}" />
    <SolidColorBrush x:Key="Brush.Highlight.2" Color="{StaticResource Color.Highlight.2}" />
    
    <!--<SolidColorBrush x:Key="Brush.Select.Foreground" Color="{StaticResource Color.Background}" />
    <SolidColorBrush x:Key="Brush.Select.Background" Color="{StaticResource Color.Foreground}" />
    <SolidColorBrush x:Key="Brush.Warning" Color="{StaticResource Color.Warning}" />
    <SolidColorBrush x:Key="Brush.Error" Color="{StaticResource Color.Error}" />
    <SolidColorBrush x:Key="Brush.Selection" Color="{StaticResource Color.Selection}" />-->



</ResourceDictionary>